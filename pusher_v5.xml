<!--
Pusher model for `Pusher-v5`, based on openai/gym/Pusher
modified by @Kallinteris-Andreas
	- reduced body.object cylinder density 0.00001 -> 0.01
	- disabled body.object sphere
-->
<mujoco model="arm3d">
    <compiler inertiafromgeom="true" angle="radian" coordinate="local"/>
    <option timestep="0.01" gravity="0 0 -9.81" iterations="20" integrator="Euler" />
  
    <default>
      <joint armature='0.04' damping="1" limited="true"/>
      <geom friction=".8 .1 .1" density="300" margin="0.002" condim="1" contype="0" conaffinity="0"/>
    </default>

    <asset>
        <mesh name="Pin" file="./Pin.obj" scale="0.07 0.07 0.07"/>
    </asset>

    <worldbody>
      <light diffuse=".5 .5 .5" pos="0 0 3" dir="0 0 -1"/>
      <geom friction=".8 .1 .1" name="table" type="plane" pos="0 0.5 -0.325" size="5 5 0.1" contype="1" conaffinity="1"/>
  
      <body name="r_shoulder_pan_link" pos="-0.5 -0.28 0">
        <geom name="e1" type="sphere" rgba="0.6 0.6 0.6 1" pos="-0.06 0.05 0.2" size="0.05" />
        <geom name="e2" type="sphere" rgba="0.6 0.6 0.6 1" pos=" 0.06 0.05 0.2" size="0.05" />
        <geom name="e1p" type="sphere" rgba="0.1 0.1 0.1 1" pos="-0.06 0.09 0.2" size="0.03" />
        <geom name="e2p" type="sphere" rgba="0.1 0.1 0.1 1" pos=" 0.06 0.09 0.2" size="0.03" />
        <geom name="sp" type="capsule" fromto="0 0 -0.4 0 0 0.2" size="0.1" conaffinity="1" contype="1" />
        <joint name="r_shoulder_pan_joint" type="hinge" pos="0 0 0" axis="0 0 1" range="-3.14 3.14" damping="1.0" />
  
        <body name="r_shoulder_lift_link" pos="0.1 0 0">
          <geom name="sl" type="capsule" fromto="0 -0.1 0 0 0.1 0" size="0.1" conaffinity="1" contype="1" />
          <joint name="r_shoulder_lift_joint" type="hinge" pos="0 0 0" axis="0 1 0" range="-1.48 1.32" damping="5.0" />
  
          <body name="r_upper_arm_roll_link" pos="0 0 0">
            <geom name="uar" type="capsule" fromto="-0.1 0 0 0.1 0 0" size="0.02" />
            <joint name="r_upper_arm_roll_joint" type="hinge" pos="0 0 0" axis="1 0 0" range="-3.14 3.14" damping="0.1" />
  
            <body name="r_upper_arm_link" pos="0 0 0">
              <geom name="ua" type="capsule" fromto="0 0 0 0.4 0 0" size="0.06" />
  
              <body name="r_elbow_flex_link" pos="0.4 0 0">
                <geom name="ef" type="capsule" conaffinity="1" contype="1" fromto="0 -0.02 0 0.0 0.02 0" size="0.06" />
                <joint name="r_elbow_flex_joint" type="hinge" pos="0 0 0" axis="0 1 0" range="-2.7 2.7" damping="0.1" />
  
                <body name="r_forearm_roll_link" pos="0 0 0">
                  <geom name="fr" type="capsule" conaffinity="1" contype="1" fromto="-0.1 0 0 0.1 0 0" size="0.02" />
                  <joint name="r_forearm_roll_joint" type="hinge" limited="true" pos="0 0 0" axis="1 0 0" damping=".1" range="-3.14 3.14"/>
  
                  <body name="r_forearm_link" pos="0 0 0">
                    <geom name="fa" type="capsule" conaffinity="1" contype="1" fromto="0 0 0 0.291 0 0" size="0.05" />
  
                    <body name="r_wrist_flex_link" pos="0.321 0 0">
                      <geom name="wf" type="capsule" fromto="0 -0.02 0 0 0.02 0" size="0.01" />
                      <joint name="r_wrist_flex_joint" type="hinge" pos="0 0 0" axis="0 1 0" range="-3.14 3.14" damping=".1" />
  
                      <body name="r_wrist_roll_link" pos="0 0 0">
                        <joint name="r_wrist_roll_joint" type="hinge" pos="0 0 0" limited="true" axis="1 0 0" damping="0.1" range="-3.14 3.14"/>
                        <body name="tips_arm" pos="0 0 0">
                          <geom name="tip_arml" type="sphere" pos="0.1 -0.1 0." size="0.01" />
                          <geom name="tip_armr" type="sphere" pos="0.1 0.1 0." size="0.01" />
                        </body>
                        <geom type="capsule" fromto="0 -0.1 0. 0.0 +0.1 0" size="0.02" contype="1" conaffinity="1" />
                        <geom type="capsule" fromto="0 -0.1 0. 0.1 -0.1 0" size="0.02" contype="1" conaffinity="1" />
                        <geom type="capsule" fromto="0 +0.1 0. 0.1 +0.1 0." size="0.02" contype="1" conaffinity="1" />
                      </body>
                    </body>
                  </body>
                </body>
              </body>
            </body>
          </body>
        </body>
      </body>
    
    <body name="object" pos="0.0 -0.05 -0.25" >
        <joint name="obj_free" type="free" damping="0.0" />
        <geom rgba="1 1 1 1" type="sphere" size="0.09" density="1370" contype="1" conaffinity="1"/>
    </body>
          
    <!-- 10-Pin Setup -->
    <!-- Row 1 -->
      <body name="pin1" pos="0.0 2.0 -0.2">
        <joint name="pin1_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>
      
      <body name="pin2" pos="-0.0762 2.132 -0.2">
        <joint name="pin2_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>
      
      <body name="pin3" pos="0.0762 2.132 -0.2">
        <joint name="pin3_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>
      
      <body name="pin4" pos="-0.1524 2.2639 -0.2">
        <joint name="pin4_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>
      
      <body name="pin5" pos="0.0 2.2639 -0.2">
        <joint name="pin5_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>
      
      <body name="pin6" pos="0.1524 2.2639 -0.2">
        <joint name="pin6_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>
      
      <body name="pin7" pos="-0.2286 2.3959 -0.2">
        <joint name="pin7_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>
      
      <body name="pin8" pos="-0.0762 2.3959 -0.2">
        <joint name="pin8_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>
      
      <body name="pin9" pos="0.0762 2.3959 -0.2">
        <joint name="pin9_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>

      <body name="pin10" pos="0.2286 2.3959 -0.2">
        <joint name="pin10_free" type="free" damping="0.0"/>
        <geom type="mesh" mesh="Pin" size="0.025" density="500" condim="4" contype="1" conaffinity="1" rgba="1 1 1 1" />
      </body>

      <body name="random" pos="100 100 0.0">
        <joint name="random" type="free" damping="1"/>
        <geom type="mesh" mesh="Pin" size="0.025" mass="1" rgba="1 1 1 1" contype="1" conaffinity="1"/>
      </body>

    <body name="goal" pos="0.0 2.37 -0.3230">
        <geom rgba="1 0 0 1" type="cylinder" size="0.08 0.001 0.1" density='0.00001' contype="0" conaffinity="0"/>
        <joint name="goal_slidey" type="slide" pos="0 0 0" axis="0 1 0" range="-10.3213 10.3" damping="0.5"/>
        <joint name="goal_slidex" type="slide" pos="0 0 0" axis="1 0 0" range="-10.3213 10.3" damping="0.5"/>
    </body>

    </worldbody>
   
    <actuator>
      <motor joint="r_shoulder_pan_joint" ctrlrange="-2.0 2.0" gear='5' ctrllimited="true" />
      <motor joint="r_shoulder_lift_joint" ctrlrange="-2.0 2.0" gear='5' ctrllimited="true" />
      <motor joint="r_upper_arm_roll_joint" ctrlrange="-2.0 2.0" gear='5' ctrllimited="true" />
      <motor joint="r_elbow_flex_joint" ctrlrange="-2.0 2.0" gear='5' ctrllimited="true" />
      <motor joint="r_forearm_roll_joint" ctrlrange="-2.0 2.0" gear='5' ctrllimited="true" />
      <motor joint="r_wrist_flex_joint" ctrlrange="-2.0 2.0" gear='5' ctrllimited="true" />
      <motor joint="r_wrist_roll_joint" ctrlrange="-2.0 2.0" gear='5' ctrllimited="true"/>
    </actuator>
  </mujoco>